{
  "compilerOptions": {
    "moduleResolution": "node",
    "declaration": true,
    "emitDeclarationOnly": true,
    "target": "es6",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "quasar"
    ]
  }
}
